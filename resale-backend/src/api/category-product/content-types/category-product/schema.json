{
  "kind": "collectionType",
  "collectionName": "category_products",
  "info": {
    "singularName": "category-product",
    "pluralName": "category-products",
    "displayName": "Category_Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "description": {
      "type": "string"
    },
    "category_product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category-product.category-product",
      "inversedBy": "parent"
    },
    "parent": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category-product.category-product",
      "mappedBy": "category_product"
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "category_products"
    },
    "SEO": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": true
    }
  }
}
